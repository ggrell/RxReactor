{
  "problemMatcher": [
    {
      "owner": "gradle",
      "pattern": [
        {
          "regexp": "^(error|quiet|warning|lifecycle|info|debug)\\s+(\\S+)\\s+(.+)$",
          "severity": 1,
          "code": 2,
          "message": 3
        },
        {
          "regexp": "^([^\\s]+):([\\d]+)$",
          "file": 1,
          "line": 2
        }
      ]
    }
  ]
}
